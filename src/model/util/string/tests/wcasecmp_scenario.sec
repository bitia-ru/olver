static bool init_string_wcasecmp_scenario(int argc, char** argv)
{
    /* Init test scenario data */
    context = getContext();

    return true;
}

static void finish_string_wcasecmp_scenario(void)
{
    /**/
}

/********************************************************************/
/**                          Test Actions                          **/
/********************************************************************/


scenario
bool string_wcasecmp_scen(void)
{

    wcscasecmp_spec(context, create_WString(convert_to_wcstring("String")), create_WString(convert_to_wcstring("striNg")));
    wcscasecmp_spec(context, create_WString(convert_to_wcstring("StringX")), create_WString(convert_to_wcstring("striNgY")));
    wcscasecmp_spec(context, create_WString(convert_to_wcstring("striNgY")), create_WString(convert_to_wcstring("Strin")));


    wcsncasecmp_spec(context, create_WString(convert_to_wcstring("String1")), create_WString(convert_to_wcstring("striNg2")), 5);
    wcsncasecmp_spec(context, create_WString(convert_to_wcstring("StringX")), create_WString(convert_to_wcstring("striNgY")), 10);
    wcsncasecmp_spec(context, create_WString(convert_to_wcstring("striN4gY")), create_WString(convert_to_wcstring("Strin3")), 6);


    return true;
}


/********************************************************************/
/**                     Test Scenario Definition                   **/
/********************************************************************/

scenario dfsm util_string_wcasecmp_scenario =
{
    .init = init_string_wcasecmp_scenario,
    .finish = finish_string_wcasecmp_scenario,
    .actions = {
        string_wcasecmp_scen,
        NULL
    }
};



